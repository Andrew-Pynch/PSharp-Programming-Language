    "\n",
    "class PSharp:\n",
    "    def __init__(self, text):\n",
    "        self.text = text\n",
    "        \n",
    "    def run(self):\n",
    "        self.lexer = Lexer(self.text)\n",
    "        lexer, tokens = self.lexer.make_tokens()\n",
    "        \n",
    "        return lexer, tokens"
